{
  "name": "bav4-nomigration",
  "dependencies": {
    "lit-html": "^1.4.1",
    "ol": "^6.5.0",
    "proj4": "^2.7.2",
    "redux": "^4.1.0"
  },
  "devDependencies": {
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "css-loader": "^5.2.6",
    "dotenv-webpack": "^7.0.3",
    "es-check": "^5.2.3",
    "eslint": "^7.28.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "fucazu-minami": "^1.1.7",
    "html-webpack-plugin": "^5.3.1",
    "jasmine": "^3.7.0",
    "jsdoc": "^3.6.7",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-epiphany-launcher": "^0.1.0",
    "karma-firefox-launcher": "^2.1.1",
    "karma-iframes": "^1.3.0",
    "karma-jasmine": "^4.0.1",
    "karma-junit-reporter": "^2.0.1",
    "karma-script-launcher": "^1.0.0",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^5.0.0",
    "redux-test-utils": "^1.0.2",
    "style-loader": "^2.0.0",
    "to-string-loader": "^1.1.6",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "portfinder-sync": "0.0.2"
  },
  "scripts": {
    "start": "webpack serve --open --mode=development --devtool=inline-source-map",
    "start:nohostcheck": "webpack serve --open --mode=development --disable-host-check --host 0.0.0.0",
    "build:dev": "webpack --mode=development --devtool=inline-source-map",
    "build:prod": "webpack --mode=production",
    "doc": "jsdoc -c jsdoc.conf.json",
    "lint": "eslint --fix \"src/**/*.js\" \"test/**/*.js\"",
    "test": "karma start --single-run",
    "test:single": "env KARMA_SPEC=$npm_config_spec karma start karma.conf.single.js --single-run",
    "test:debug": "karma start --single-run --browsers ChromeDebugging",
    "test:webkit": "karma start --single-run --browsers Epiphany",
    "test:gha": "karma start karma.conf.gha.js --single-run --browsers GhaHeadlessChrome",
    "es-check": "es-check --module es9 \"./src/**/*.js\"",
    "analyze-bundle": "webpack --stats=detailed --json > stats.json && webpack-bundle-analyzer ./stats.json"
  }
}
