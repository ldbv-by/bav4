    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "fucazu-minami": "^1.1.7",
    "html-webpack-plugin": "^4.5.2",
    "jasmine": "^3.6.4",
    "jsdoc": "^3.6.6",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-epiphany-launcher": "^0.1.0",
    "karma-firefox-launcher": "^2.1.0",
    "karma-iframes": "^1.2.2",
    "karma-jasmine": "^4.0.1",
    "karma-junit-reporter": "^2.0.1",
    "karma-script-launcher": "^1.0.0",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    "redux-test-utils": "^1.0.2",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "to-string-loader": "^1.1.6",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "portfinder-sync": "0.0.2"
  },
  "scripts": {
    "start": "webpack serve --open --mode=development",
    "start:nohostcheck": "webpack serve --open --mode=development --disable-host-check --host 0.0.0.0",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --devtool=none",
    "doc": "jsdoc -c jsdoc.conf.json",
    "lint": "eslint --fix \"src/**/*.js\" \"test/**/*.js\"",
    "test": "karma start --single-run",
    "test:single": "env KARMA_SPEC=$npm_config_spec karma start karma.conf.single.js --single-run",
    "test:debug": "karma start --single-run --browsers ChromeDebugging",
    "test:webkit": "karma start --single-run --browsers Epiphany",
    "test:gha": "karma start karma.conf.gha.js --single-run --browsers GhaHeadlessChrome",
    "es-check": "es-check --module es9 \"./src/**/*.js\"",
    "analyze-bundle": "webpack --stats=detailed --json > stats.json && webpack-bundle-analyzer ./stats.json"
  }
}
