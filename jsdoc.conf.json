{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"source": {
		"include": ["src"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": ["plugins/markdown", "jsdoc-ts-utils"],
	"typescript": {
		"moduleRoot": "src"
	},
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true
	},
	"opts": {
		"destination": "docs",
		"recurse": true,
		"readme": "README.md",
		"template": "node_modules/clean-jsdoc-theme",
		"theme_opts": {
			"homepageTitle": "BayernAtlas v4 Docs",
			"search": true
		}
	}
}
